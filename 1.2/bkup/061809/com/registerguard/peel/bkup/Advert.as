package com.registerguard.peel {		import flash.display.MovieClip;	import flash.net.URLRequest;	import flash.display.Loader;	import flash.events.Event;	import flash.events.ProgressEvent;		public class Advert extends MovieClip {				private var _params:Params;		private var _load:Load;				/**		* Advert()		*        About: Class constructor.		*      Returns: Nothing.		*/		public function Advert() {						trace('Advert() instantiated...');						init();					};				private function init():void {						trace('Advert init()...');						_params = new Params(this);			_load = new Load(this);						_load.loadThis(_params.getParam("advert", "http://assets.registerguard.com/demos/as3/advert.swf"));						_load.addEventListener(Load.LOAD_COMPLETE, onLoaded, false, 0, true);					};				private function onLoaded(e:Event):void {						trace('Load.LOAD_COMPLETE');						_load.removeEventListener(Load.LOAD_COMPLETE, onLoaded); // GC.						var clip:MovieClip = _load.clip;			clip.stop();			this.addChild(clip);					}			};	};
package com.registerguard.peel {		import flash.display.MovieClip;	import flash.display.DisplayObject;	import flash.events.Event;		public class Peel extends MovieClip {				private var _stager:Stager;		private var _cookie:Cookie;		private var _peelMc:MovieClip;		private var _maskMc:MovieClip;		private var _rewind:Boolean;		private var _flag:Boolean;				public function Peel():void {						init();					};				private function init():void {						_stager = new Stager(this);						_cookie = new Cookie("cookie");						peeler();					};				private function peeler():void {						this.buttonMode = true;						_flag = false;						_peelMc = this.peel_mc;			_maskMc = this.mask_mc;						if (_cookie.cookieSet() == false) {								if (_cookie.cookieCheck() == true) {										_cookie.cookieDelete();									}								this.addEventListener(Event.ENTER_FRAME, onEnter, false, 0, true);							} else {								_peelMc.play();				_maskMc.play();							}					}				private function onEnter(e:Event):void {						if (_peelMc.currentFrame == 1) {								_flag = false;				_rewind = false;				_peelMc.play();				_maskMc.play();							} else if (!_flag && (_peelMc.currentLabel != 'loop')) {								_peelMc.gotoAndPlay('loop');				_maskMc.gotoAndPlay('loop');							} else if (_rewind) {								_peelMc.prevFrame();				_maskMc.prevFrame();							}					};			};	};
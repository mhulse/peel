package com.registerguard.peel {		import flash.display.MovieClip;	import flash.net.URLRequest;	import flash.display.Loader;	import flash.events.Event;	import flash.events.ProgressEvent;		public class Tease extends MovieClip {				private var _params:Params;		private var _load:Load;				/**		* Tease()		*        About: Class constructor.		*      Returns: Nothing.		*/		public function Tease() {						trace('Tease() instantiated...');						init();					};				private function init():void {						trace('Tease init()...');						_params = new Params(this);			_load = new Load(this);						_load.loadThis(_params.getParam("tease", "http://assets.registerguard.com/demos/as3/tease.swf"));					};			};	};